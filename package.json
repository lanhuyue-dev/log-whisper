{
  "name": "log-whisper",
  "private": true,
  "version": "1.0.0",
  "description": "LogWhisper - 强大的日志分析桌面工具 (基于React+Tauri+Rust架构)",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "start": "tauri dev",
    "tauri:dev": "tauri dev",
    "tauri:dev:static": "npm run build:css:prod && npx vite build && tauri dev --config src-tauri/tauri.static.conf.json",
    "tauri:build": "tauri build",
    "build:css": "tailwindcss -i ./src/style.css -o ./src/style.css --watch",
    "build:css:prod": "tailwindcss -i ./src/style.css -o ./src/style.css --minify",
    "build:styles": "tailwindcss -i ./src/style.css -o ./src/style.css --minify",
    "watch:styles": "tailwindcss -i ./src/style.css -o ./src/style.css --watch",
    "build:rust": "cd src-tauri && cargo build --release",
    "package": "tauri build",
    "dist": "npm run build",
    "dist:win": "npm run build:css:prod && tauri build --target x86_64-pc-windows-msvc",
    "dist:mac": "npm run build:css:prod && tauri build --target universal-apple-darwin",
    "dist:linux": "npm run build:css:prod && tauri build --target x86_64-unknown-linux-gnu",
    "clean": "rm -rf dist/ node_modules/ src-tauri/target/",
    "test": "npm run test:tauri",
    "test:tauri": "cd src-tauri && cargo test"
  },
  "keywords": [
    "log",
    "analysis",
    "desktop",
    "tauri",
    "rust",
    "react",
    "parser",
    "developer-tools",
    "logging",
    "monitoring"
  ],
  "author": {
    "name": "LogWhisper Team",
    "email": "team@log-whisper.com",
    "url": "https://github.com/lanhuyue-dev/log-whisper"
  },
  "license": "Apache-2.0",
  "homepage": "https://github.com/lanhuyue-dev/log-whisper",
  "repository": {
    "type": "git",
    "url": "https://github.com/lanhuyue-dev/log-whisper.git"
  },
  "bugs": {
    "url": "https://github.com/lanhuyue-dev/log-whisper/issues"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "@tauri-apps/api": "^1.6.0",
    "clsx": "^2.0.0",
    "lucide-react": "^0.293.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.1",
    "tailwind-merge": "^2.0.0"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.19",
    "@tauri-apps/cli": "^1.5.11",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.1.1",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.53.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.2.2",
    "vite": "^4.5.0"
  }
}
